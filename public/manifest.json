{
  "manifest_version": 2,
  "name": "ONES Chrome Helper",
  "description": "yoooooooooooooo.",
  "version": "1.1",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "favicon2020_x16.png",
    "48": "favicon2020_x1024.png",
    "64": "favicon2020_x1024.png",
    "128": "favicon2020_x1024.png"
  },
  "browser_action": {
    "default_icon": "favicon2020_x16.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://dev.localhost/*", "https://dev.myones.net/*"],
      "js": ["content_scripts.js"],
      "run_at": "document_end"
    }
  ]
}