{
  "manifest_version": 2,
  "name": "ONES Chrome Helper",
  "description": "yoooooooooooooo.",
  "version": "1.0",
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_icon": "favicon2020.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://dev.myones.net/*", "http://dev.localhost:3000/*"],
      "js": ["src/inject.js"],
      "run_at": "document_end"
    }
  ]
}